{
  "name": "work-focus",
  "productName": "Work Focus",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "dist/main/main.js",
  "scripts": {
    "build:main": "tsc -p tsconfig.json",
    "build:renderer": "esbuild src/index.tsx --bundle --outfile=dist/main/renderer/index.js --platform=browser --target=es2020 --format=esm",
    "copy-public": "cp public/index.html dist/main/renderer/index.html",
    "build": "npm run build:main && npm run build:renderer && npm run copy-public",
    "start": "NODE_ENV=production npm run build && electron .",
    "dev:main": "tsc -w -p tsconfig.json",
    "dev:renderer": "esbuild src/index.tsx --bundle --outfile=dist/main/renderer/index.js --platform=browser --target=es2020 --format=esm --watch",
    "start:dev": "npm run build:renderer && npm run copy-public && concurrently --kill-others-on-fail \"npm run dev:main\" \"npm run dev:renderer\" \"wait-on dist/main/renderer/index.html && electron .\"",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux"
  },
  "keywords": [],
  "author": {
    "name": "khoahd",
    "email": "hdkhoa1302@gmail.com"
  },
  "license": "MIT",
  "build": {
    "appId": "com.workfocus.app",
    "productName": "Work Focus",
    "electronCompile": false,
    "icon": "build/icon.png",
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png",
      "category": "Office"
    },
    "dmg": {
      "backgroundColor": "#f0f0f0",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "files": [
      "dist/**/*",
      "package.json",
      "assets/**/*"
    ]
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "work_focus"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "workfocus"
      }
    }
  },
  "dependencies": {
    "@google/genai": "^1.5.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@tiptap/core": "^2.1.13",
    "@tiptap/extension-bold": "^2.1.13",
    "@tiptap/extension-bullet-list": "^2.1.13",
    "@tiptap/extension-document": "^2.1.13",
    "@tiptap/extension-hard-break": "^2.1.13",
    "@tiptap/extension-heading": "^2.1.13",
    "@tiptap/extension-history": "^2.1.13",
    "@tiptap/extension-italic": "^2.1.13",
    "@tiptap/extension-link": "^2.1.13",
    "@tiptap/extension-list-item": "^2.1.13",
    "@tiptap/extension-ordered-list": "^2.1.13",
    "@tiptap/extension-paragraph": "^2.1.13",
    "@tiptap/extension-strike": "^2.1.13",
    "@tiptap/extension-text": "^2.1.13",
    "@tiptap/extension-underline": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^5.1.0",
    "i18next": "^25.2.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "node-notifier": "^10.0.1",
    "ps-list": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.5.3",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.2",
    "recharts": "^2.15.3",
    "uuid": "^11.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/electron": "^1.4.38",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.0",
    "@types/node-notifier": "^8.0.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^9.1.2",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "electron-compile": "^6.4.4",
    "electron-forge": "^5.2.4",
    "electron-reload": "^2.0.0-alpha.1",
    "esbuild": "^0.19.12",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  }
}
